    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-istanbul": "3.0.4",
