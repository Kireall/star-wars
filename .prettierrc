{
  "trailingComma": "es5",
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "tabWidth": 4,
  "printWidth": 120,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": ["^[^@.]",".*(?<![.]css)$","^.+.css$"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}